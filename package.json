{
  "name": "check-version",
  "version": "0.0.1",
  "description": "Inspection Service version",
  "main": "lib/index.js",
  "bin": "./bin/check-version",
  "scripts": {
    "test-cov": "istanbul cover ./node_modules/mocha/bin/_mocha -- -t 5000 --recursive  -R spec test/",
    "test": "mocha --reporter spec --timeout 5000 --recursive test/",
    "check": "fecs check src/ test/",
    "compile": "babel --optional runtime --loose all --stage 0 --modules common src/ --out-dir lib/",
    "watch": "npm run compile -- --watch",
    "prepublish": "npm run compile",
    "start": "node start.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xuexb/check-version.git"
  },
  "author": "xiaowu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/xuexb/check-version/issues"
  },
  "homepage": "https://github.com/xuexb/check-version#readme",
  "devDependencies": {
    "babel": "5.8.21",
    "fecs": "*",
    "istanbul": "^0.4.1",
    "mocha": "^2.3.4"
  },
  "dependencies": {
    "babel-runtime": "^5.8.20",
    "colors": "^1.1.2",
    "commander": "^2.9.0",
    "key-cache": "0.x.x",
    "marked": "^0.3.5",
    "node-schedule": "^1.0.0",
    "nodemailer": "^2.1.0",
    "request": "2.x.x"
  }
}
